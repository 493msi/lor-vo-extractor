<Window x:Class="LoRAudioExtractor.UI.LicenseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoRAudioExtractor.UI"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" xmlns:ui="clr-namespace:LoRAudioExtractor.UI"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        TextElement.FontWeight="Medium"
        TextElement.FontSize="14"
        FontFamily="{materialDesign:MaterialDesignFont}"
        mc:Ignorable="d"
        Title="Licenses" Height="640" Width="800" Icon="/assets/icon.ico" WindowStartupLocation="CenterOwner">
    <Grid>
        <ScrollViewer>
            <TextBlock x:Name="LicenseContent" TextWrapping="Wrap" FontFamily="Consolas" Margin="5"/>
        </ScrollViewer>
    </Grid>
</Window>
